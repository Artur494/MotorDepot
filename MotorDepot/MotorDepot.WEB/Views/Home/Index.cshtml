@{
    ViewBag.Title = "Home Page";
}

@CustomHelper.RenderAll()

@section scripts
{
    <script>
        $('.alert-dismissible').click(function() {

            var key = $(this).find('.session-key').text();

            $.ajax({
                type: "POST",
                url: `/Home/SessionAlertDeny?sessionKey=${key}`
            });
        });
    </script>
}